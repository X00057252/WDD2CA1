<% include ./partials/header %>
<% include ./partials/sidebar %>

<div class="user-list">
    <div class="user-list-title">User list</div>
    <table>
        <tr>
            <th>username</th>
            <th>first name</th>
            <th>last name</th>
            <th>email</th>
            <th>address</th>
        </tr>
        <% users.forEach(function(user){ %>
        <tr>
            <td><%=user.username%></td>
            <td><%=user.fname%></td>
            <td><%=user.lname%></td>
            <td><%=user.email%></td>
            <td><%=user.address%></td>
        </tr>
        <% }) %>
    </table>
</div>


<% include ./partials/footer %>